@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <title>Hospitals </title>
    <meta content="Hospital Dashboard" name="description" />
    <meta content="Hospitals" name="author" />
    <link rel="shortcut icon" href="~/Content/Theme/assets/images/favicon.ico">
    @Styles.Render("~/Theme/css")
</head>
<body>
    <div id="wrapper">
        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left">
                <a href="index.html" class="logo">
                    <span class="logo-light">
                        <img src="~/Content/Theme/assets/images/logo.svg" alt="" class="logo-brand" width="160" />
                    </span>
                    <span class="logo-sm">
                        <img src="~/Content/Theme/assets/images/logo-small.svg" alt="" class="logo-brand" width="50" />
                    </span>
                </a>
            </div>

            <nav class="navbar-custom d-flex justify-content-between">
                <ul class="list-inline menu-left mb-0">
                    <li class="float-left ml-3">
                        <button class="button-menu-mobile open-left">
                            <i class="mdi mdi-menu"></i>
                        </button>
                    </li>
                </ul>
                <ul class="navbar-right list-inline float-right mb-0 mr-4 mt-2 d-flex align-items-center">
                    <li class="dropdown">
                        <a class="nav-link nav-link-label" href="#" data-toggle="dropdown" aria-expanded="true">
                            <i class="fa fa-bell" aria-hidden="true"></i>
                            <span class="badge badge-pill badge-danger badge-up">5</span>
                        </a>
                    </li>
                    <li class="dropdown notification-list">
                        <div class="dropdown notification-list">
                            <a class="d-flex align-items-end dropdown-toggle nav-link arrow-none nav-user"
                               data-toggle="dropdown" href="#" role="button" aria-haspopup="false"
                               aria-expanded="false">
                                <img src="~/Content/Theme/assets/images/avatar.jpg" alt="user" class="rounded-circle mr-2">
                                <div class="pro-text">
                                    <p class="m-0 p-0 text-dark fw-600">Mr. Ravi</p>
                                    <small class="text-muted">System Admin</small>
                                </div>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                                <!-- item-->
                                <a class="dropdown-item" href="#">
                                    <i class="mdi mdi-account-circle"></i> Change
                                    Password
                                </a>
                                <!-- <div class="dropdown-divider"></div> -->
                                <a class="dropdown-item text-danger" href="#">
                                    <i class="mdi mdi-power text-danger"></i>
                                    Logout
                                </a>
                            </div>
                        </div>
                    </li>
                </ul>
            </nav>

        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu">
            <!--- Sidemenu -->
            <div id="sidebar-menu">
                <ul class="metismenu" id="side-menu">
                    <li>
                        <a href="javascript:void(0);" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/admin.svg"
                                 alt=""><span>
                                Admin<span class="float-right menu-arrow"><i class="mdi mdi-chevron-right"></i></span>
                            </span>
                        </a>
                        <ul class="submenu">
                            <li><a href="branch.html">Branch Management</a></li>
                            <li><a href="role.html">Role Management</a></li>
                            <li><a href="department.html">Department Management</a></li>
                            <li><a href="staff.html">Staff Management</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript:void(0);" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/doctor.svg"
                                 alt=""><span>
                                Doctor<span class="float-right menu-arrow"><i class="mdi mdi-chevron-right"></i></span>
                            </span>
                        </a>
                        <ul class="submenu">
                            <li><a href="specialist.html">Specialist</a></li>
                            <li><a href="junior.html">Junior Doctor</a></li>
                        </ul>
                    </li>

                    <li>
                        <a href="" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/slot.svg" alt=""><span>
                                Slot
                            </span>
                        </a>
                    </li>

                    <li>
                        <a href="javascript:void(0);" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/traffic.svg"
                                 alt=""><span>
                                Traffic<span class="float-right menu-arrow">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                            </span>
                        </a>
                        <ul class="submenu">
                            <li><a href="service-tariff-management.html">Services</a></li>
                            <li><a href="packages.html">Packages & Offers</a></li>
                        </ul>
                    </li>

                    <li>
                        <a href="" class="waves-effect"><img src="~/Content/Theme/assets/images/sms.svg" alt=""><span>SMS</span></a>
                    </li>

                    <li>
                        <a href="javascript:void(0);" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/general.svg"
                                 alt=""><span>
                                General<span class="float-right menu-arrow">
                                    <i class="mdi mdi-chevron-right"></i>
                                </span>
                            </span>
                        </a>
                        <ul class="submenu">
                            <li><a href="">Brand Upload</a></li>
                            <li>
                                <a href="javascript:void(0);">
                                    Master Data <span class="float-right menu-arrow">
                                        <i class="mdi mdi-chevron-right"></i>
                                </a>
                                <ul class="submenu">
                                    <li><a href="">Countries</a></li>
                                    <li><a href="areas.html">Areas</a></li>
                                    <li><a href="regions.html">Regions</a></li>
                                </ul>
                            </li>
                            <li><a href="salutation.html">Salutation</a></li>
                        </ul>
                    </li>

                    <li>
                        <a href="token-management.html" class="waves-effect">
                            <img src="~/Content/Theme/assets/images/token.svg" alt=""><span>
                                Token
                            </span>
                        </a>
                    </li>
                </ul>

            </div>
            <div class="clearfix"></div>
            <!-- Sidebar -left -->
        </div>
        <!-- Left Sidebar End -->
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <!-- ============================================================== -->
        <!-- Content here -->
        <!-- ============================================================== -->

        <div class="content-page content">
            <div class="wrap-content p-4">
                <div class="container-fluid mt-5">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Theme")
    @RenderSection("scripts", required: false)
    <script src="~/Content/Theme/assets/js/jquery.min.js"></script>
    <script>$("a.my-tool-tip").tooltip();</script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
</body>
</html>